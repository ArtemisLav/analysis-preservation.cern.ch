[
  {
    "key": "analysis_context",
    "type": "fieldset",
    "htmlClass": "custom-panel",
    "items": [
      {
        "key": "analysis_context.name"
      },
      {
        "key": "analysis_context.preapproval_date",
        "items": [
          {
            "key": "analysis_context.preapproval_date.name",
            "title": "Name",
            "type": "select",
            "titleMap": [
              {  
                "value": "schedule",
                "name": "Scheduled for (please specify in Additional comments)"
              },
              {  
                "value": "date_range",
                "name": "Approximate date range (please specify in Additional comments)"
              },
              {  
                "value": "undecided",
                "name": "Completely undecided"
              },
              {  
                "value": "not_approval",
                "name": "Not seeking approval"
              }
            ]
          },
          {
            "key": "analysis_context.preapproval_date.comments",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "analysis_context.email"
      },
      {
        "key": "analysis_context.group",
        "items": [
          {
            "key": "analysis_context.group.name",
            "title": "Name",
            "type": "select",
            "titleMap": [
              {  
                "value": "bph",
                "name": "B physics and quarkonia (BPH)"
              },
              {  
                "value": "b2g",
                "name": "Beyond-two-generations (B2G)"
              },
              {  
                "value": "exo",
                "name": "Exotica (EXO)"
              },
              {  
                "value": "fsq",
                "name": "Forward and Small-x QCD physics (FSQ)"
              },
              {  
                "value": "hig",
                "name": "Higgs physics (HIG)"
              },
              {  
                "value": "hin",
                "name": "Heavy ions physics (HIN)"
              },
              {  
                "value": "smp",
                "name": "Standard Model physics (SMP)"
              },
              {  
                "value": "sus",
                "name": "Supersymmetry (SUS)"
              },
              {  
                "value": "top",
                "name": "Top physics (TOP)"
              },
              {  
                "value": "other",
                "name": "Other (Please specify in Additional comments)"
              }
            ]
          },
          {
            "key": "analysis_context.group.comments",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "analysis_context.cadi_id"
      },
      {
        "key": "analysis_context.analysis_title"
      }
    ]
  },
  {
    "key": "general",
    "type": "fieldset",
    "items": [
      {
        "key": "general.faq_awareness",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      },
      {
        "key": "general.forum_awareness",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      },
      {
        "key": "general.forum_subscription",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      },
      {
        "key": "general.past_interaction",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      },
      {
        "key": "general.meetings_awareness",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      }
    ]
  },
  {
    "key": "multivariate",
    "type": "fieldset",
    "items": [
      {
        "key": "multivariate.twiki_read",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      },
      {
        "key": "multivariate.usage",
        "type": "radios",
        "titleMap": [
          { "value": false, "name": "No" },
          { "value": true, "name": "Yes" }
        ]
      },
      {
        "key": "multivariate.software",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.software.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "tmva", "name": "TMVA" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.software.comments",
            "condition": "model['multivariate']['software']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]  
      },
      {
        "key": "multivariate.discriminant_type",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.discriminant_type.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "ann", "name": "Artificial Neural Network" },
              { "value": "boosted", "name": "Boosted Decision Trees" },
              { "value": "fisher", "name": "Fisher Discriminant" },
              { "value": "likelehoods", "name": "Product of projected likelihoods" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.discriminant_type.comments",
            "condition": "model['multivariate']['discriminant_type']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.mva_output_usage",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.mva_output_usage.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "output_cut", "name": "We cut on the output to improve S/B" },
              { "value": "output_fit", "name": "We fit the output distribution to get the signal fraction" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.mva_output_usage.comments",
            "condition": "model['multivariate']['mva_output_usage']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.input_checks",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.input_checks.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "1d", "name": "We study all 1D distributions" },
              { "value": "2d", "name": "We study all 2D distributions" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.input_checks.comments",
            "condition": "model['multivariate']['input_checks']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.correlation_studies",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.correlation_studies.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "matrix", "name": "We study the correlation matrix between input variables" },
              { "value": "nonlinear", "name": "We also study dependencies beyond linear correlation (please specify in Additional comments)" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.correlation_studies.comments",
            "condition": "model['multivariate']['correlation_studies']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.input_selection",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.input_selection.name",
            "type": "radios",
            "notitle": true,
            "titleMap": [
              { "value": "yes", "name": "Yes, we use the correlation matrix and remove highly correlated variables" },
              { "value": "no", "name": "No, we keep the complete list" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.input_selection.comments",
            "condition": "model['multivariate']['input_selection']['name'] == 'other'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.null_pdf",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.null_pdf.name",
            "type": "radios",
            "notitle": true,
            "titleMap": [
              { "value": "yes_not_relevant", "name": "Yes, but this does not apply to my analysis" },
              { "value": "yes_need_to_check", "name": "Yes, and I need to check whether this applies to my analysis" },
              { "value": "yes_large_sample", "name": "Yes, I make training samples large enough and I checked that this does not occur" },
              { "value": "no", "name": "We do not study that" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.null_pdf.comments",
            "condition": "model['multivariate']['null_pdf']['name'] == 'other'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.background_mix",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.background_mix.name",
            "type": "radios",
            "notitle": true,
            "titleMap": [
              { "value": "undecided", "name": "Undecided" },
              { "value": "yes", "name": "Yes" },
              { "value": "no", "name": "Does not apply: we only have one background process" },
              { "value": "other", "name": "No (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.background_mix.comments",
            "condition": "model['multivariate']['background_mix']['name'] == 'other'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.overtraining",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.overtraining.name",
            "type": "radios",
            "notitle": true,
            "titleMap": [
              { "value": "yes_partition", "name": "We divide the sample into two subsamples: one used exclusively for training, and the other for all subsequent analysis (optimizations, analysis plots, measurements using the discriminant, etc.)." },
              { "value": "no", "name": "We do not perform any checks" },
              { "value": "other", "name": "We do perform checks (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.overtraining.comments",
            "condition": "model['multivariate']['overtraining']['name'] == 'other'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "multivariate.robustness",
        "type": "fieldset",
        "items": [
          {
            "key": "multivariate.robustness.name",
            "type": "radios",
            "notitle": true,
            "titleMap": [
              { "value": "yes", "name": "Yes" },
              { "value": "no", "name": "We do not perform any checks" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "multivariate.robustness.comments",
            "condition": "model['multivariate']['robustness']['name'] == 'other'",
            "type": "textarea"
          }
        ]
      }
    ]
  },
  {
    "key": "data_fitting",
    "type": "fieldset",
    "items": [
      {
        "key": "data_fitting.usage",
        "type": "radios",
        "titleMap": [
          { "value": "yes", "name": "Yes" },
          { "value": "no", "name": "No" },
          { "value": "undecided", "name": "Undecided" }
        ]
      },
      {
        "key": "data_fitting.functional_form",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.functional_form.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "templates", "name": "Histograms/Templates" },
              { "value": "parametric", "name": "Parametric curves/pdfs" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "data_fitting.functional_form.comments",
            "condition": "model['data_fitting']['functional_form']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.fitting_model_choice",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.fitting_model_choice.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "template_mc", "name": "Histograms/Templates from MC" },
              { "value": "template_sideband", "name": "Histograms/Templates from a data sideband" },
              { "value": "theory_curves", "name": "Theory curve(s)" },
              { "value": "theory_inspired_curves", "name": "Theory-inspired curve(s)" },
              { "value": "adhoc_curves", "name": "Ad-hoc curve(s)" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "data_fitting.fitting_model_choice.comments",
            "condition": "model['data_fitting']['fitting_model_choice']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.test_stats",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.test_stats.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "chi_square", "name": "Chi-square" },
              { "value": "binned", "name": "Binned likelihood" },
              { "value": "unbinned", "name": "Unbinned likelihood" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "data_fitting.test_stats.reason",
            "type": "textarea"
          },
          {
            "key": "data_fitting.test_stats.comments",
            "condition": "model['data_fitting']['test_stats']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.goodness_test",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.goodness_test.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "undecided", "name": "Undecided" },
              { "value": "yes", "name": "Yes" },
              { "value": "no", "name": "No" }
            ]
          },
          {
            "key": "data_fitting.goodness_test.p-value",
            "condition": "model['data_fitting']['goodness_test']['name'] =='undecided' || model['data_fitting']['goodness_test']['name'] =='yes'",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "0001-0010", "name": "0.1%-1%" },
              { "value": "0010-0050", "name": "1%-5%" },
              { "value": "0050-0990", "name": "5%-99%" },
              { "value": "0990-0999", "name": "99%-99.9%" },
              { "value": "0-0001", "name": "<0.1%" },
              { "value": "0999-1000", "name": ">99.9%" },
              { "value": "dont_know", "name": "Don't know" }
            ]
          },
          {
            "key": "data_fitting.goodness_test.comments",
            "condition": "model['data_fitting']['goodness_test']['name'] =='undecided' || model['data_fitting']['goodness_test']['name'] =='yes'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.toy_pull_distribution",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.toy_pull_distribution.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "no_not_checked", "name": "We do not check that" },
              { "value": "no_not_used", "name": "No, but we do not use the uncertainty returned by the fit anyway" },
              { "value": "yes_unbiased", "name": "Yes, and the result is that the method is unbiased" },
              { "value": "yes_found_something", "name": "Yes, and there are some deviations we account for (please specify in Additional comments)" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "data_fitting.toy_pull_distribution.comments",
            "condition": "model['data_fitting']['toy_pull_distribution']['name'] == 'other' || model['data_fitting']['toy_pull_distribution']['name'] == 'yes_found_something'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.covariance_matrix",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.covariance_matrix.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "not_relevant", "name": "Does not apply: We do not use the parameters / uncertainties elsewhere." },
              { "value": "yes", "name": "Yes" },
              { "value": "no", "name": "No" }
            ]
          },
          {
            "key": "data_fitting.covariance_matrix.comments",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.background_shape",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.background_shape.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "not_relevant", "name": "Does not apply: We do not perform this kind of fitting" },
              { "value": "yes_checked", "name": "We perform robustness checks to show that we are insensitive to the choice of the functional form" },
              { "value": "yes_assigned_from_difference", "name": "We tried different options and assign a systematic uncertainty from the difference" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "data_fitting.background_shape.comments",
            "condition": "model['data_fitting']['background_shape']['name'] == 'other'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.fit_function",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.fit_function.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "not_relevant", "name": "Does not apply: We do not use such a function for fitting" },
              { "value": "chose_okish", "name": "We choose what looks more or less Ok" },
              { "value": "robustness_checks", "name": "We perform robustness checks to show that we are insensitive to this choice" },
              { "value": "conservative", "name": "We tried different options and chose the most conservative one" },
              { "value": "formal", "name": "We applied a formal decision procedure (such as Fisher F-Test, LR test, please specify in Additional comments)" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]
          },
          {
            "key": "data_fitting.fit_function.comments",
            "condition": "model['data_fitting']['fit_function']['name'] == 'other' || model['data_fitting']['fit_function']['name'] == 'formal'",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "data_fitting.parameter_boundaries",
        "condition": "model['data_fitting']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "data_fitting.parameter_boundaries.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "not_checked", "name": "We do not check that" },
              { "value": "no", "name": "No" },
              { "value": "yes_unused", "name": "Yes, but we do not use the uncertainty" },
              { "value": "yes_checked", "name": "Yes, and we check that the error calculation is robust anyway" },
              { "value": "yes_trusted", "name": "Yes, and we trust the errors from the fitter without further checks" }
            ]
          },
          {
            "key": "data_fitting.parameter_boundaries.comments",
            "type": "textarea"
          }
        ]
      }
    ]
  },
  {
    "key": "confidence",
    "type": "fieldset",
    "items": [
      {
        "key": "confidence.usage",
        "type": "radios",
        "titleMap": [
          { "value": "undecided", "name": "Undecided" },
          { "value": "no", "name": "No" },
          { "value": "yes", "name": "Yes" }
        ]
      },
      {
        "key": "confidence.citation",
        "condition": "model['confidence']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "confidence.citation.name",
            "notitle": true,
            "type": "radios",
            "titleMap": [
              { "value": "undecided", "name": "Completely undecided" },
              { "value": "cite_limit", "name": "We will only cite a limit" },
              { "value": "cite_interval", "name": "We will only cite a two-sided interval" },
              { "value": "to_decise", "name": "We will decide that after looking at data" },
              { "value": "cite_both", "name": "We will cite both." },
              { "value": "not_relevant", "name": "Does not apply: we use a unified interval construction approach (Feldman-Cousins)" }
            ]    
          },
          {
            "key": "confidence.citation.comments",
            "type": "textarea"
          }
        ]
      },
      {
        "key": "confidence.atlas_cms_agreement",
        "condition": "model['confidence']['usage'] !== 'no'",
        "type": "radios",
        "titleMap": [
          { "value": "not_relevant", "name": "Does not apply: we do not perform limit computation" },
          { "value": "no", "name": "No" },
          { "value": "yes", "name": "Yes" }
        ]
      },
      {
        "key": "confidence.support_awareness",
        "condition": "model['confidence']['usage'] !== 'no'",
        "type": "radios",
        "titleMap": [
          { "value": "not_relevant", "name": "Does not apply: we do not perform limit computation" },
          { "value": "no", "name": "No" },
          { "value": "yes", "name": "Yes" }
        ]
      },
      {
        "key": "confidence.recommendation_awareness",
        "condition": "model['confidence']['usage'] !== 'no'",
        "type": "radios",
        "titleMap": [
          { "value": "not_relevant", "name": "Does not apply: we do not perform limit computation" },
          { "value": "no", "name": "No" },
          { "value": "yes", "name": "Yes" }
        ]
      },
      {
        "key": "confidence.approaches",
        "condition": "model['confidence']['usage'] !== 'no'",
        "type": "fieldset",
        "items": [
          {
            "key": "confidence.approaches.name",
            "notitle": true,
            "type": "checkboxes",
            "titleMap": [
              { "value": "freq_limits", "name": "Frequentist limits" },
              { "value": "freq_intervals", "name": "Frequentist intervals" },
              { "value": "freq_limits_mod", "name": "Modified frequentist limits (CLs)" },
              { "value": "prof_lh_limits", "name": "Profile likelihood limits" },
              { "value": "prof_lh_intervals", "name": "Profile likelihood intervals" },
              { "value": "unified", "name": " Unified Approach (Feldman-Cousins)" },
              { "value": "bayes_flat", "name": "Bayesian limits/intervals, with a flat prior for the parameter of interest" },
              { "value": "byes_ref", "name": "Bayesian limits/intervals, with a reference prior for the parameter of interest" },
              { "value": "other", "name": "Other (please specify in Additional comments)" }
            ]    
          },
          {
            "key": "confidence.approaches.comments",
            "condition": "model['confidence']['approaches']['name'].indexOf('other') !== -1",
            "type": "textarea"
          }
        ]
      },
      {
