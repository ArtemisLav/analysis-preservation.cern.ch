{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "id": "physics_object",
  "type": "object",
  "properties": {
    "object": {
      "title": "Object",
      "type": "string",
      "enum": [
        "",
        "electron",
        "muon",
        "tau",
        "jet",
        "bjet",
        "photon",
        "track",
        "vertex",
        "MET",
        "HT"
      ]
    },
    "number": {
      "type": "object",
      "properties": {
        "sign": {
          "type": "string",
          "title": "<, >, =, <=, >=",
          "enum": ["<", ">", "=", "<=", ">="],
          "default": "="
        },
        "number": {
          "type": "number",
          "title": "Number"
        }
      }
    },
    "electron_type": {
      "type": "string",
      "title": "Electron type",
      "enum": ["GsfElectron"]
    },
    "muon_type": {
      "type": "string",
      "title": "Muon type",
      "enum": ["PFMuon", "GlobalMuon", "TrackerMuon"]
    },
    "tau_type": {
      "type": "string",
      "title": "Tau type",
      "enum": ["PFTau"]
    },
    "jet_type": {
      "type": "string",
      "title": "Jet type",
      "enum": [
        "AK4PF",
        "AK5PF",
        "AK6PF",
        "AK7PF",
        "AK8PF",
        "AK4Calo",
        "AK5Calo",
        "AK6Calo",
        "AK7Calo",
        "AK8Calo",
        "KT4PF",
        "KT5PF",
        "KT6PF",
        "KT7PF",
        "KT8PF",
        "KT4Calo",
        "KT5Calo",
        "KT6Calo",
        "KT7Calo",
        "KT8Calo"
      ]
    },
    "photon_type": {
      "type": "string",
      "title": "Photon type",
      "enum": ["Photon", "Conversion"]
    },
    "met_type": {
      "type": "string",
      "title": "MET type",
      "enum": ["PFMET", "CaloMET"]
    },
    "track_type": {
      "type": "string",
      "title": "Track type",
      "enum": ["Heavy-Ion", "pp"]
    },
    "sel_criteria": {
      "type": "string",
      "title": "Selection Criteria",
      "enum": ["tight", "medium", "loose", "other"]
    },
    "pt_cut": {
      "type": "number",
      "title": "pt cut"
    },
    "pt_cut_min": {
      "type": "number",
      "title": "pt cut min"
    },
    "pt_cut_max": {
      "type": "number",
      "title": "pt cut max"
    },
    "eta_cut": {
      "type": "number",
      "title": "eta cut"
    },
    "eta_cut_min": {
      "type": "number",
      "title": "eta cut min"
    },
    "eta_cut_max": {
      "type": "number",
      "title": "eta cut max"
    }
  }
}
